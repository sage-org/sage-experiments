
SELECT DISTINCT  ?subject ?predicate ?object
WHERE
  {   { <http://dbpedia.org/resource/Lost_%28TV_series%29> ?predicate ?object }
    UNION
      { ?subject ?predicate <http://dbpedia.org/resource/Lost_%28TV_series%29> }
    FILTER ( ( lang(?object) = "en" ) || ( lang(?object) = "" ) )
  }
ORDER BY ?predicate ?subject
OFFSET  0
LIMIT   200