      UNION
        { ?resource foaf:page <http://en.wikipedia.org/wiki/Safari_Books_Online> . ?resource foaf:page ?uri .
        ?resource rdfs:label ?wtitle .
        OPTIONAL
          { ?resource rdfs:comment ?comment
