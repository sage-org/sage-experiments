         }
      UNION
        { ?resource foaf:page <http://en.wikipedia.org/wiki/Internet_forum> . ?resource foaf:page ?uri .
        ?resource rdfs:label ?wtitle .
        OPTIONAL
          { ?resource rdfs:comment ?comment
            FILTER langMatches(lang(?comment), "en")
          }
        OPTIONAL
